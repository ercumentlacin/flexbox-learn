{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-restricted-exports": 0,
        "react/react-in-jsx-scope": 0,
        "react/function-component-definition": 0,
        "react/jsx-props-no-spreading": 0,
        "react/prop-types": 0,
        "no-console": 0,
        "no-restricted-syntax": 0
    }
    
}
